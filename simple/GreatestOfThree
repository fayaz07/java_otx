import java.util.*;

public class GreatestOfThree{
  public static void main (String args[]){
    Scanner scan = new Scanner(System.in);
    int a = scan.nextInt();
    int b = scan.nextInt();
    int c = scan.nextInt();
    
    int res = a>b?a:b;
    res = res>c?res:c;
    
    System.out.println("Greatest number is " + res);
    
  }
}
